<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="USE_CUSTOM_SOFT_WRAP_INDENT" value="true" />
    <option name="CUSTOM_SOFT_WRAP_INDENT" value="2" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_RIGHT_MARGIN_SHOWN" value="false" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="JetEditorOptions">
    <option name="donTShowConversionDialog" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\btodo\b.*">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="default" useCustomColors="false" case-sensitive="false" pattern="\bfixme\b.*">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="important" useCustomColors="true" case-sensitive="false" pattern="\bstopship\b.*">
      <option name="FOREGROUND" value="ffffff" />
      <option name="BACKGROUND" value="990000" />
      <option name="FONT_TYPE" value="1" />
      <option name="EFFECT_COLOR" value="990000" />
      <option name="ERROR_STRIPE_COLOR" value="ff0000" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
  </component>
  <component name="XmlEditorOptions">
    <option name="showCssInlineColorPreview" value="true" />
    <option name="tagTreeHighlightingEnabled" value="false" />
  </component>
</application>