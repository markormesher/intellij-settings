<application>
  <component name="SqlEditorOptions">
    <option name="concatenateStringsOnEnter" value="true" />
    <option name="qualificationType" value="On collisions" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="\bstopship\b.*">
      <option name="FOREGROUND" value="ffffff" />
      <option name="BACKGROUND" value="990000" />
      <option name="FONT_TYPE" value="1" />
      <option name="EFFECT_COLOR" value="990000" />
      <option name="ERROR_STRIPE_COLOR" value="ff0000" />
      <option name="EFFECT_TYPE" value="1" />
    </pattern>
  </component>
</application>